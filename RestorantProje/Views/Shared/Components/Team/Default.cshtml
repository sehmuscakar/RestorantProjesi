
@model IEnumerable<Repository.Entities.Team>

<!-- Team Start -->
<div class="container-xxl pt-5 pb-3">
    <div class="container">
        <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
            <h5 class="section-title ff-secondary text-center text-primary fw-normal">Personlerimiz</h5>
            <h1 class="mb-5">Lezzet Restoran</h1>
        </div>
        <div class="row g-4">
            @foreach (var item in Model)
            {
                
            <div class="col-lg-3 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                <div class="team-item text-center rounded overflow-hidden">
                    <div class="rounded-circle overflow-hidden m-4">
                            <img class="img-fluid" src="~/Img/@item.Image" alt="">
                    </div>
                    <h5 class="mb-0">@item.Name</h5>
                    <small>@item.Expert</small>
                    <div class="d-flex justify-content-center mt-3">
                        <a class="btn btn-square btn-primary mx-1" href=""><i class="fab fa-facebook-f"></i></a>
                        <a class="btn btn-square btn-primary mx-1" href=""><i class="fab fa-phone-alt"></i></a>
                        <a class="btn btn-square btn-primary mx-1" href="@item.SocialMedia"><i class="fab fa-instagram"></i></a>
                    </div>
                </div>
            </div>
            }
        </div>
    </div>
</div>
<!-- Team End -->